{
  "name": "node-front-template",
  "version": "1.0.0",
  "description": "template for used for front end based on node.js",
  "main": "lib/app.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint src/**/*.js",
    "develop": "./node_modules/.bin/nodemon lib/app.js",
    "release": "pm2 ",
    "build": "./node_modules/.bin/babel src -d lib -w -s"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/greedlab/node-front-template.git"
  },
  "keywords": [
    "node.js",
    "front",
    "koa2",
    "template"
  ],
  "author": "Bell <bell@greedlab.com> ",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/greedlab/node-front-template/issues"
  },
  "homepage": "https://github.com/greedlab/node-front-template#readme",
  "engines": {
    "node": ">= 4"
  },
  "dependencies": {
    "art-template": "^3.0.3",
    "koa": "^2.0.0",
    "koa-bodyparser": "^3.1.0",
    "koa-favicon": "^2.0.0",
    "koa-logger": "^2.0.0",
    "koa-router": "^7.0.1",
    "koa-static": "^3.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.0.4",
    "babel-plugin-transform-async-to-module-method": "^6.8.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "eslint": "^2.11.1",
    "eslint-plugin-react": "^5.1.1",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-imagemin": "^3.0.1",
    "gulp-livereload": "^3.8.1",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^1.5.3",
    "gulp-watch": "^4.3.6",
    "imagemin-pngquant": "^5.0.0",
    "nodemon": "^1.9.2"
  }
